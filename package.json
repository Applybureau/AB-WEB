{
  "name": "apply-bureau-backend",
  "version": "1.0.0",
  "description": "Professional Application & Interview Advisory System Backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup": "node scripts/setup.js",
    "create-admin": "node scripts/create-admin.js",
    "setup-database": "node scripts/setup-database.js",
    "quick-test": "node scripts/quick-deploy-test.js",
    "test-api": "node scripts/test-api.js",
    "test-endpoints": "node scripts/test-all-endpoints.js",
    "test-real-world": "node scripts/real-world-test.js",
    "test-final": "node scripts/final-comprehensive-test.js",
    "deploy-check": "node scripts/deploy-check.js",
    "health-check": "node scripts/health-check.js",
    "test-live": "node scripts/test-live-backend.js",
    "lint": "echo 'Linting...' && echo 'No linter configured'",
    "audit": "npm audit --audit-level=moderate",
    "security-check": "npm audit && npm run deploy-check",
    "full-test": "npm run test-endpoints && npm run test",
    "production-ready": "npm run deploy-check && npm run test && npm run test-final"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "googleapis": "^128.0.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "resend": "^2.1.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.8.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
